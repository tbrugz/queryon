<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<schemaModel>
    <!--
    Example of QueryOn Access Log Table
    -->
    <table>
        <name>QON_ACCESS_LOG</name>
        <schemaName>QUERYON</schemaName>
        <columns>
            <name>server_name</name>
            <type>VARCHAR</type>
            <columnSize>100</columnSize>
        </columns>
        <columns>
            <name>server_port</name>
            <type>INTEGER</type>
        </columns>
        <columns>
            <name>server_protocol</name>
            <type>VARCHAR</type>
            <columnSize>20</columnSize>
        </columns>
        <columns>
            <name>remote_addr</name>
            <type>VARCHAR</type>
            <columnSize>100</columnSize>
            <nullable>false</nullable>
        </columns>
        <columns>
            <name>status_code</name>
            <type>INTEGER</type>
            <nullable>false</nullable>
        </columns>
        <columns>
            <name>username</name>
            <type>VARCHAR</type>
            <columnSize>100</columnSize>
        </columns>
        <columns>
            <name>request_method</name>
            <type>VARCHAR</type>
            <columnSize>20</columnSize>
            <nullable>false</nullable>
        </columns>
        <columns>
            <name>timestamp_ini</name>
            <type>TIMESTAMP</type>
            <nullable>false</nullable>
        </columns>
        <columns>
            <name>elapsed_ms</name>
            <type>INTEGER</type>
            <nullable>false</nullable>
        </columns>
        <columns>
            <name>request_uri</name>
            <type>VARCHAR</type>
            <columnSize>1000</columnSize>
            <nullable>false</nullable>
        </columns>
        <columns>
            <name>query_string</name>
            <type>VARCHAR</type>
            <columnSize>1000</columnSize>
        </columns>
        <columns>
            <name>header_referer</name>
            <type>VARCHAR</type>
            <columnSize>1000</columnSize>
        </columns>
        <columns>
            <name>header_useragent</name>
            <type>VARCHAR</type>
            <columnSize>1000</columnSize>
        </columns>
    </table>

    <!-- <sqlDialect>?</sqlDialect> -->
</schemaModel>
