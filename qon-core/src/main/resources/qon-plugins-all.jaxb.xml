<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<schemaModel>
    <!--
    Example of a model to use with QOnTables, QOnExecs, QOnQueries & QOnPages with sensible defaults.
    
    Default roles:
    - admin
    - dev
    - user
    -->
    <table>
        <name>QON_TABLES</name>
        <schemaName>QUERYON</schemaName>
        <columns>
            <name>SCHEMA_NAME</name>
            <columSize>100</columSize>
            <nullable>true</nullable>
            <pk>false</pk>
            <type>VARCHAR</type>
        </columns>
        <columns>
            <name>NAME</name>
            <columSize>100</columSize>
            <nullable>false</nullable>
            <pk>true</pk>
            <type>VARCHAR</type>
        </columns>
        <columns>
            <name>COLUMN_NAMES</name>
            <columSize>400</columSize>
            <nullable>true</nullable>
            <pk>false</pk>
            <remarks>Names of the columns (separated by comma)</remarks>
            <type>VARCHAR</type>
        </columns>
        <columns>
            <name>DEFAULT_COLUMN_NAMES</name>
            <columSize>400</columSize>
            <nullable>true</nullable>
            <pk>false</pk>
            <remarks>Names of the default columns (separated by comma)</remarks>
            <type>VARCHAR</type>
        </columns>
        <columns>
            <name>REMARKS</name>
            <columSize>400</columSize>
            <nullable>true</nullable>
            <pk>false</pk>
            <remarks>Short description of the table</remarks>
            <type>VARCHAR</type>
        </columns>
        <columns>
            <name>ROLES_SELECT</name>
            <columSize>1000</columSize>
            <nullable>true</nullable>
            <pk>false</pk>
            <remarks>Roles allowed to SELECT (separated by "|") - if null ALL are allowed</remarks>
            <type>VARCHAR</type>
        </columns>
        <columns>
            <name>ROLES_INSERT</name>
            <columSize>1000</columSize>
            <nullable>true</nullable>
            <pk>false</pk>
            <remarks>Roles allowed to INSERT (separated by "|") - if null NONE are allowed</remarks>
            <type>VARCHAR</type>
        </columns>
        <columns>
            <name>ROLES_UPDATE</name>
            <columSize>1000</columSize>
            <nullable>true</nullable>
            <pk>false</pk>
            <remarks>Roles allowed to UPDATE (separated by "|") - if null NONE are allowed</remarks>
            <type>VARCHAR</type>
        </columns>
        <columns>
            <name>ROLES_DELETE</name>
            <columSize>1000</columSize>
            <nullable>true</nullable>
            <pk>false</pk>
            <remarks>Roles allowed to DELETE (separated by "|") - if null NONE are allowed</remarks>
            <type>VARCHAR</type>
        </columns>
        <columns>
            <name>ROLES_INSERT_COLUMNS</name>
            <columSize>1000</columSize>
            <nullable>true</nullable>
            <pk>false</pk>
            <remarks>Insert permission on columns (&lt;column&gt;:&lt;role&gt;[comma&lt;role&gt;][|...])</remarks>
            <type>VARCHAR</type>
        </columns>
        <columns>
            <name>ROLES_UPDATE_COLUMNS</name>
            <columSize>1000</columSize>
            <nullable>true</nullable>
            <pk>false</pk>
            <remarks>Update permission on columns (&lt;column&gt;:&lt;role&gt;[comma&lt;role&gt;][|...])</remarks>
            <type>VARCHAR</type>
